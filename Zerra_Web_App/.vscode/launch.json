    "version": "0.2.0",
    "configurations": [
        {
            "name": "my_web_app",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart", // Entry point of your app
            "deviceId": "chrome"       // Specify Chrome or Brave as the device
        },
        {
            "name": "my_web_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main.dart",
            "deviceId": "chrome"
        },
        {
            "name": "my_web_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main.dart",
            "deviceId": "chrome"
        }
    ]
}
